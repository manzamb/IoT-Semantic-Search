    <!-- http://purl.obolibrary.org/obo/ENVO_01000175 -->

    <owl:Class rdf:about="&obo;ENVO_01000175">
        <rdfs:label rdf:datatype="&xsd;string">woodland biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_00000446"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2013-04-07T14:26:46Z</oboInOwl:creation_date>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">A woodland biome is a terrestrial biome which includes, across its entire spatial extent, woody plants spaced sufficiently far apart to allow light penetration to support communities of herbaceous plants or shrubs living closer to the woodland floor.</obo:IAO_0000115>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">ENVO</oboInOwl:hasOBONamespace>
        <oboInOwl:id rdf:datatype="&xsd;string">ENVO:01000175</oboInOwl:id>
        <rdfs:comment rdf:datatype="&xsd;string">Preliminary definition.</rdfs:comment>
