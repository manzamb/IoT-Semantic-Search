    <!-- http://purl.obolibrary.org/obo/ENVO_01000048 -->

    <owl:Class rdf:about="&obo;ENVO_01000048">
        <rdfs:label rdf:datatype="&xsd;string">ocean biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_00000447"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2010-03-15T11:23:23Z</oboInOwl:creation_date>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">ENVO</oboInOwl:hasOBONamespace>
        <oboInOwl:id rdf:datatype="&xsd;string">ENVO:01000048</oboInOwl:id>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">The ocean biome comprises major bodies of saline water, principal components of the hydrosphere. Approximately 71% of the Earth&apos;s surface is covered by ocean, a continuous body of water that is customarily divided into several principal oceans and smaller seas. More than half of this area is over 3,000 metres (9,800 ft) deep. Average oceanic salinity is around 35 parts per thousand (ppt) (3.5%), and nearly all seawater has a salinity in the range of 30 to 38 ppt.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:datatype="&xsd;string">pbuttigieg</oboInOwl:created_by>
    </owl:Class>
    <owl:Axiom>
        <oboInOwl:hasDbXref rdf:datatype="&xsd;string">PLB:plb</oboInOwl:hasDbXref>
        <owl:annotatedTarget rdf:datatype="&xsd;string">The ocean biome comprises major bodies of saline water, principal components of the hydrosphere. Approximately 71% of the Earth&apos;s surface is covered by ocean, a continuous body of water that is customarily divided into several principal oceans and smaller seas. More than half of this area is over 3,000 metres (9,800 ft) deep. Average oceanic salinity is around 35 parts per thousand (ppt) (3.5%), and nearly all seawater has a salinity in the range of 30 to 38 ppt.</owl:annotatedTarget>
        <oboInOwl:hasDbXref rdf:datatype="&xsd;string">Wikipedia:Ocean</oboInOwl:hasDbXref>
        <owl:annotatedSource rdf:resource="&obo;ENVO_01000048"/>
        <owl:annotatedProperty rdf:resource="&obo;IAO_0000115"/>
    </owl:Axiom>
    


    <!-- http://purl.obolibrary.org/obo/ENVO_01000049 -->

    <owl:Class rdf:about="&obo;ENVO_01000049">
        <rdfs:label rdf:datatype="&xsd;string">marine coral reef biome</rdfs:label>
