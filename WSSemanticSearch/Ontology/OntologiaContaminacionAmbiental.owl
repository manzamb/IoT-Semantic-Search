    <!-- http://purl.obolibrary.org/obo/ENVO_01000041 -->

    <owl:Class rdf:about="&obo;ENVO_01000041">
        <rdfs:label rdf:datatype="&xsd;string">neritic sea surface microlayer biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_01000032"/>
