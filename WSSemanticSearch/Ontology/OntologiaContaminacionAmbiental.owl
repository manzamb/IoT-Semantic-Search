    <!-- http://purl.obolibrary.org/obo/ENVO_01000036 -->

    <owl:Class rdf:about="&obo;ENVO_01000036">
        <rdfs:label rdf:datatype="&xsd;string">oceanic mesopelagic zone biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_01000033"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2010-03-15T11:21:03Z</oboInOwl:creation_date>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">ENVO</oboInOwl:hasOBONamespace>
        <oboInOwl:id rdf:datatype="&xsd;string">ENVO:01000036</oboInOwl:id>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">The oceanic mesopelagic zone biome comprises regions of the marine water column that occur offshore, beyond a continental shelf. This biome extends through the water column from approximately 200-1000 m depth - the maximal depth of light penetration in all but the clearest oligotrophic ocean regions. In comparison to the epipelagic zone, dominant forms of pelagic organisms do not change very much; however, there are marked changes in species composition and the proportion of smaller-sized individuals decreases.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:datatype="&xsd;string">pbuttigieg</oboInOwl:created_by>
    </owl:Class>
