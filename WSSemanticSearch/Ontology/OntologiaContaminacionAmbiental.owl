    <owl:Class rdf:about="&obo;ENVO_01000128">
        <rdfs:label rdf:datatype="&xsd;string">dilution basin mediterranean sea biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_01000047"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2010-03-19T09:32:59Z</oboInOwl:creation_date>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">ENVO</oboInOwl:hasOBONamespace>
        <oboInOwl:id rdf:datatype="&xsd;string">ENVO:01000128</oboInOwl:id>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">The dilution basin mediterranean sea biome comprises expressions of the mediterranean sea biome with lower salinity than the surrounding ocean due to freshwater gains such as rainfall and rivers. Water exchange consists of outflow of the fresher mediterranean water in the upper layer and inflow of the saltier oceanic water in the lower layer of the channel. Renewal of deep water may not be sufficient to supply oxygen to the bottom.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:datatype="&xsd;string">pbuttigieg</oboInOwl:created_by>
    </owl:Class>
    <owl:Axiom>
        <oboInOwl:hasDbXref rdf:datatype="&xsd;string">PLB:plb</oboInOwl:hasDbXref>
        <owl:annotatedTarget rdf:datatype="&xsd;string">The dilution basin mediterranean sea biome comprises expressions of the mediterranean sea biome with lower salinity than the surrounding ocean due to freshwater gains such as rainfall and rivers. Water exchange consists of outflow of the fresher mediterranean water in the upper layer and inflow of the saltier oceanic water in the lower layer of the channel. Renewal of deep water may not be sufficient to supply oxygen to the bottom.</owl:annotatedTarget>
        <oboInOwl:hasDbXref rdf:datatype="&xsd;string">Wikipedia:Mediterranean_sea</oboInOwl:hasDbXref>
        <owl:annotatedSource rdf:resource="&obo;ENVO_01000128"/>
        <owl:annotatedProperty rdf:resource="&obo;IAO_0000115"/>
    </owl:Axiom>
    


    <!-- http://purl.obolibrary.org/obo/ENVO_01000174 -->

    <owl:Class rdf:about="&obo;ENVO_01000174">
        <rdfs:label rdf:datatype="&xsd;string">forest biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_00000446"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2013-04-07T14:26:46Z</oboInOwl:creation_date>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">A forest biome is a terrestrial biome which includes, across its entire spatial extent, densely packed vegetation which strongly limits light penetration to the forest floor.</obo:IAO_0000115>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">ENVO</oboInOwl:hasOBONamespace>
        <oboInOwl:id rdf:datatype="&xsd;string">ENVO:01000174</oboInOwl:id>
