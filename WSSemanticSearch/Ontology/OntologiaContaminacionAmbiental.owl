    <!-- http://purl.obolibrary.org/obo/ENVO_01000004 -->

    <owl:Class rdf:about="&obo;ENVO_01000004">
        <rdfs:label rdf:datatype="&xsd;string">concentration basin mediterranean sea biome</rdfs:label>
