        <oboInOwl:hasDbXref rdf:datatype="&xsd;string">Wikipedia:Sea_surface_microlayer</oboInOwl:hasDbXref>
        <owl:annotatedSource rdf:resource="&obo;ENVO_01000041"/>
        <owl:annotatedProperty rdf:resource="&obo;IAO_0000115"/>
