    <!-- http://purl.obolibrary.org/obo/ENVO_01000045 -->

    <owl:Class rdf:about="&obo;ENVO_01000045">
        <rdfs:label rdf:datatype="&xsd;string">epeiric sea biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_00000447"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2010-03-15T11:23:23Z</oboInOwl:creation_date>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">ENVO</oboInOwl:hasOBONamespace>
        <oboInOwl:id rdf:datatype="&xsd;string">ENVO:01000045</oboInOwl:id>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">The epeiric sea (also known as an epicontinental sea) biome comprises a shallow seas that extend over part of a continent. Epeiric seas are usually associated with the marine transgressions of the geologic  past, which have variously been due to either global eustatic sea level changes, local tectonic deformation, or both, and are occasionally semi-cyclic.</obo:IAO_0000115>
        <oboInOwl:hasExactSynonym rdf:datatype="&xsd;string">epicontinental sea biome</oboInOwl:hasExactSynonym>
        <oboInOwl:created_by rdf:datatype="&xsd;string">pbuttigieg</oboInOwl:created_by>
    </owl:Class>
    <owl:Axiom>
