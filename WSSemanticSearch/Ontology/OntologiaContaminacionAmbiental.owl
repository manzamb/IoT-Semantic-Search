    <owl:Class rdf:about="&obo;ENVO_01000123">
        <rdfs:label rdf:datatype="&xsd;string">marine sponge reef biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_01000029"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2010-03-17T04:43:05Z</oboInOwl:creation_date>
