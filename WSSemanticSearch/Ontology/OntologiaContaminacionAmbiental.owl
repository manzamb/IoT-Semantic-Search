    <!-- http://purl.obolibrary.org/obo/ENVO_01000038 -->

    <owl:Class rdf:about="&obo;ENVO_01000038">
        <rdfs:label rdf:datatype="&xsd;string">oceanic abyssopelagic zone biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_01000033"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2010-03-15T11:21:03Z</oboInOwl:creation_date>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">ENVO</oboInOwl:hasOBONamespace>
        <oboInOwl:id rdf:datatype="&xsd;string">ENVO:01000038</oboInOwl:id>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">The abyssopelagic zone biome comprises the marine water column below the bathypelagic zone biome at approximately 2500 -2700 m and extends either to the hadal pelagic zone biome (at approximately 6000 m) or to the benthopelagic zone biome within ca. 100 m of the seafloor. The depth of 2700 m may be critical for physiological reasons as the change in hydrostatic pressure across this depth may preclude organism migration.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:datatype="&xsd;string">pbuttigieg</oboInOwl:created_by>
    </owl:Class>
    <owl:Axiom>
        <oboInOwl:hasDbXref rdf:datatype="&xsd;string">ISBN:044482619X</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref rdf:datatype="&xsd;string">PLB:plb</oboInOwl:hasDbXref>
        <owl:annotatedTarget rdf:datatype="&xsd;string">The abyssopelagic zone biome comprises the marine water column below the bathypelagic zone biome at approximately 2500 -2700 m and extends either to the hadal pelagic zone biome (at approximately 6000 m) or to the benthopelagic zone biome within ca. 100 m of the seafloor. The depth of 2700 m may be critical for physiological reasons as the change in hydrostatic pressure across this depth may preclude organism migration.</owl:annotatedTarget>
        <owl:annotatedSource rdf:resource="&obo;ENVO_01000038"/>
        <owl:annotatedProperty rdf:resource="&obo;IAO_0000115"/>
    </owl:Axiom>
    


