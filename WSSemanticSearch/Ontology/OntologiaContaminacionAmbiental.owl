    </owl:Axiom>
    


    <!-- http://purl.obolibrary.org/obo/ENVO_01000047 -->

    <owl:Class rdf:about="&obo;ENVO_01000047">
        <rdfs:label rdf:datatype="&xsd;string">mediterranean sea biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_00000447"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2010-03-15T11:23:23Z</oboInOwl:creation_date>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">ENVO</oboInOwl:hasOBONamespace>
        <oboInOwl:id rdf:datatype="&xsd;string">ENVO:01000047</oboInOwl:id>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">The mediterranean sea biome comprises mostly enclosed seas that have limited exchange of deep water with outer oceans and where the water circulation is dominated by salinity and temperature differences rather than winds.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:datatype="&xsd;string">pbuttigieg</oboInOwl:created_by>
