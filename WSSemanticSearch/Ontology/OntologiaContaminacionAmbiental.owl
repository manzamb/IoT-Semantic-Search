        <owl:annotatedSource rdf:resource="&obo;ENVO_01000053"/>
        <owl:annotatedProperty rdf:resource="&obo;IAO_0000115"/>
    </owl:Axiom>
    


    <!-- http://purl.obolibrary.org/obo/ENVO_01000054 -->

    <owl:Class rdf:about="&obo;ENVO_01000054">
        <rdfs:label rdf:datatype="&xsd;string">marine basaltic hydrothermal vent biome</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;ENVO_01000030"/>
        <oboInOwl:creation_date rdf:datatype="&xsd;string">2010-03-15T11:29:37Z</oboInOwl:creation_date>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">A marine basaltic hydrothermal vent biome comprises regions of the marine hydrothermal vent biome hosted on basaltic rocks. Basalt generally has a composition of 45 to 55 wt % SiO2, 2 to 6 wt % total alkalis, 0.5 to 2.0 wt % TiO2, 5 to 14 wt % FeO and 14 wt % or more Al2O3. Contents of CaO are commonly near 10 wt %, those of MgO commonly in the range 5 to 12 wt %.</obo:IAO_0000115>
        <oboInOwl:hasOBONamespace rdf:datatype="&xsd;string">ENVO</oboInOwl:hasOBONamespace>
        <oboInOwl:id rdf:datatype="&xsd;string">ENVO:01000054</oboInOwl:id>
        <oboInOwl:created_by rdf:datatype="&xsd;string">pbuttigieg</oboInOwl:created_by>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedTarget rdf:datatype="&xsd;string">A marine basaltic hydrothermal vent biome comprises regions of the marine hydrothermal vent biome hosted on basaltic rocks. Basalt generally has a composition of 45 to 55 wt % SiO2, 2 to 6 wt % total alkalis, 0.5 to 2.0 wt % TiO2, 5 to 14 wt % FeO and 14 wt % or more Al2O3. Contents of CaO are commonly near 10 wt %, those of MgO commonly in the range 5 to 12 wt %.</owl:annotatedTarget>
